{
  "name": "@shopgate/pwa-common-commerce",
  "version": "0.2.0",
  "description": "Shopgate's commerce library for PWAs",
  "repository": "shopgate/pwa-common-commerce",
  "bugs": "https://github.com/shopgate/pwa-common-commerce/issues",
  "homepage": "https://github.com/shopgate/pwa-common-commerce",
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint --parser babel-eslint --ignore-path .gitignore --ignore-path .eslintignore --ext .js --ext .jsx .",
    "precommit": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "prebuild": "rimraf ./dist",
    "build": "NODE_ENV=production babel ./ --out-dir ./dist --ignore tests,spec.js,spec.jsx,__snapshots__,.eslintrc.js,jest.config.js,dist,coverage,node_modules",
    "postbuild": "cp ./{package.json,package-lock.json,CHANGELOG.md,LICENSE.md,README.md} ./dist/",
    "version": "npm run build",
    "postversion": "git push && git push --tags && cd dist && npm publish --access public"
  },
  "lint-staged": {
    "concurrent": false,
    "linters": {
      "*.js": "eslint --parser babel-eslint --ignore-path .gitignore --ignore-path .eslintignore --ext .js --ext .jsx ."
    }
  },
  "pre-commit": [
    "precommit"
  ],
  "dependencies": {
    "recompose": "^0.26.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@shopgate/eslint-config": "^1.0.2",
    "@shopgate/pwa-common": "^0.2.0",
    "@shopgate/pwa-core": "^0.1.7",
    "@shopgate/pwa-unit-test": "^1.2.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-module-resolver": "3.0.0-beta.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "coveralls": "^3.0.0",
    "eslint-import-resolver-babel-module": "4.0.0-beta.1",
    "lint-staged": "^4.3.0",
    "lodash": "^4.17.4",
    "pre-commit": "^1.2.2",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
    "lodash": "4.x",
    "@shopgate/pwa-core": "0.1.x",
    "@shopgate/pwa-common": "0.1.x"
  }
}
